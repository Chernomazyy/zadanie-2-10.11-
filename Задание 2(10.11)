using System;

namespace project3
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = Convert.ToInt32(Console.ReadLine());
            int[] list = new int[n];
       

            int max = 0;
            int min;
            for(int i =0;i< n;i++)
            {
                list[i] = Convert.ToInt32(Console.ReadLine());
                
            }
            min = list[list.Length - 1];
            foreach(int item in list)
            {
                if(item > max)
                { max = item; }
                if(item < min)
                { min = item; }
            }
            for(int i = 0;i < n;i ++)
            {
               if (list[i] == max)
                { list[i] = min; }
               else if (list[i]==min)
                { list[i] = max; }
            }
            foreach (int item in list)
            { Console.Write(item + " "); }
            
        }
       
    }
}
